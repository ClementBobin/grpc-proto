{
  "name": "grpc-proto",
  "version": "1.0.0",
  "description": "",
  "main": "dist/client.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:client": "node dist/client.js",
    "start:server": "node dist/server.js",
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev:client": "ts-node -r tsconfig-paths/register src/client.ts",
    "dev:server": "ts-node -r tsconfig-paths/register src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "verify": "npm run build && npm test",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "postinstall": "prisma generate",
    "db:deploy": "npx prisma migrate deploy && npx prisma generate",
    "generate:api-key": "ts-node -r tsconfig-paths/register scripts/generate-api-key.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prisma": "^6.17.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@grpc/proto-loader": "^0.8.0",
    "@prisma/client": "^6.17.1",
    "zod": "^4.1.12"
  }
}
